<%- include('template/cabecera', {tituloWeb:'crear nueva mascota'}) %>

<div class="container">
  <h1>Crear</h1>

  <form action="/mascotas" method="POST">
    <input
      type="text"
      placeholder="agregar nombre"
      class="form-control my-2"
      name="nombre"
    />
    <input
      type="text"
      placeholder="agregar descripcion"
      class="form-control my-2"
      name="descripcion"
    />
    <button class="btn btn-success col-12" type="submit">agregar</button>
  </form>
</div>

